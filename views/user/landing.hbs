<section class="product spad pt-5">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<ul class="filter__controls">
					{{!-- <li class="active" data-filter="*">Best Sellers</li> --}}
					<li class="active" data-filter=".new-arrivals">New Arrivals</li>
					<li data-filter=".hot-sales">Top Priced</li>
				</ul>
			</div>
		</div>
		<div class="row product__filter">
			{{#each newproduct}}
			<div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix new-arrivals">

				<div class="product__item">
					<div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
						<span class="label text-success"
							style="box-shadow: rgba(0, 0, 0, 0.15) 2.4px 2.4px 3.2px;">-{{this.productOffer}}%</span>
						<ul class="product__hover">
							{{#if ../user}}

							<li><a>
									<button onclick="addToWishlist('{{this._id}}')">

										<img style="background: none;"
											src="data:image/png;base64,"
											alt="" data-pagespeed-url-hash="935450538"
											onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></button></a>
							</li>
							{{else}}
							<li><a href="/login">



									<img style="background: none;"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="935450538"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
								</a>
							</li>
							{{/if}}

							<li><a href="/product-view/{{this._id}}">

									<img style="background: none"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="3756334862"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li>
						</ul>
					</div>
					<div class="product__item__text">
						<h6>{{this.game}}</h6>
						{{#if ../user}}
						<a class="add-cart"><button class="text-danger" onclick="addToCart('{{this._id}}')">+ Add To
								Cart</button></a>
						{{else}}
						<a href="/login" class="add-cart"><button class="text-danger">+ Add To
								Cart</button></a>
						{{/if}}
						<div class="rating text-muted"><strike>
								{{this.actualPrice}}</strike>
							{{!-- <i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i> --}}
						</div>
						<h5>₹{{this.offerPrice}}</h5>
						<div class="product__color__select">
							<span class="label text-light"
								style="font-size: 12px;box-shadow: rgba(0, 0, 0, 0.2) 0px 12px 28px 0px, rgba(0, 0, 0, 0.1) 0px 2px 4px 0px, rgba(255, 255, 255, 0.05) 0px 0px 0px 1px inset;background: #6676;">{{this.stock}}pcs
								Left</span>
						</div>
					</div>
				</div>

			</div>
			{{/each}}
			{{#each topPriced}}
			<div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix hot-sales">

				<div class="product__item">
					<div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
						<span class="label text-success"
							style="box-shadow: rgba(0, 0, 0, 0.15) 2.4px 2.4px 3.2px;">-{{this.productOffer}}%</span>
						<ul class="product__hover">

							{{#if ../user}}

							<li><a>
									<button onclick="addToWishlist('{{this._id}}')">

										<img style="background: none;"
											src="data:image/png;base64,"
											alt="" data-pagespeed-url-hash="935450538"
											onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></button></a>
							</li>
							{{else}}
							<li><a href="/login">



									<img style="background: none;"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="935450538"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
								</a>
							</li>
							{{/if}}


							<li><a href="/product-view/{{this._id}}">

									<img style="background: none"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="3756334862"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li>
						</ul>
					</div>
					<div class="product__item__text">
						<h6>{{this.game}}</h6>
						{{#if ../user}}
						<a class="add-cart"><button class="text-danger" onclick="addToCart('{{this._id}}')">+ Add To
								Cart</button></a>
						{{else}}
						<a href="/login" class="add-cart"><button class="text-danger">+ Add To
								Cart</button></a>
						{{/if}}
						<div class="rating text-muted"><strike>
								{{this.actualPrice}}</strike>
							{{!-- <i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i> --}}
						</div>
						<h5>₹{{this.offerPrice}}</h5>
						<div class="product__color__select">
							<span class="label text-light"
								style="font-size: 12px;box-shadow: rgba(0, 0, 0, 0.2) 0px 12px 28px 0px, rgba(0, 0, 0, 0.1) 0px 2px 4px 0px, rgba(255, 255, 255, 0.05) 0px 0px 0px 1px inset;background: #6676;">{{this.stock}}pcs
								Left</span>
						</div>
					</div>
				</div>

			</div>
			{{/each}}
			
		</div>
	</div>
</section>



<section class="latest spad">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div class="section-title">
					<span>Latest News</span>
					<h2></h2>
				</div>
			</div>
		</div>
		<div class="row">
			{{#each newFeed}}
