<div class="position-fixed w-25 d-none" id="success"
	style="left:530px;  background-color: black; border-radius:10px; z-index: 1;">
	<p class="text-center mt-3" style="color: white;">Product Added to Cart</p>
</div>
<div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
	<div class="carousel-indicators">
		<button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
			aria-current="true" aria-label="Slide 1"></button>
		<button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
			aria-label="Slide 2"></button>
		<button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
			aria-label="Slide 3"></button>
	</div>
	<div class="carousel-inner">
		{{#each bannerData}}
		<div class="carousel-item active">
			<img src="{{this.image}}" class="d-block w-100" alt="...">
			<div class="carousel-caption d-none d-md-block">
				<h5>{{this.description}}</h5>
				<p>NEW ACTION COMIN SOON</p>
			</div>
		</div>
		{{/each}}

	</div>
	<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
		<span class="carousel-control-prev-icon" aria-hidden="true"></span>
		<span class="visually-hidden">Previous</span>
	</button>
	{{!-- <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
		data-bs-slide="next">
		<span class="carousel-control-next-icon" aria-hidden="true"></span>
		<span class="visually-hidden">Next</span> --}}
	</button>
</div>




<link rel="stylesheet" href="/stylesheets/boostrapfileFomTemplate.css">
<link rel="stylesheet" href="/stylesheets/templateStyle.css">

<div id="preloder">
	<div class="loader"></div>
</div>




<section class="product spad pt-5">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<ul class="filter__controls">
					{{!-- <li class="active" data-filter="*">Best Sellers</li> --}}
					<li class="active" data-filter=".new-arrivals">New Arrivals</li>
					<li data-filter=".hot-sales">Top Priced</li>
				</ul>
			</div>
		</div>
		<div class="row product__filter">
			{{#each newproduct}}
			<div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix new-arrivals">

				<div class="product__item">
					<div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
						<span class="label text-success"
							style="box-shadow: rgba(0, 0, 0, 0.15) 2.4px 2.4px 3.2px;">-{{this.productOffer}}%</span>
						<ul class="product__hover">
							{{#if ../user}}

							<li><a>
									<button style="background: none;border: none;" onclick="addToWishlist('{{this._id}}')">

										<img style="background: none;"
											src="data:image/png;base64,"
											alt="" data-pagespeed-url-hash="935450538"
											onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></button></a>
							</li>
							{{else}}
							<li><a href="/login">



									<img style="background: none;"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="935450538"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
								</a>
							</li>
							{{/if}}

							<li><a href="/product-view/{{this._id}}">

									<img style="background: none"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="3756334862"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li>
						</ul>
					</div>
					<div class="product__item__text">
						<h6>{{this.game}}</h6>
						{{#if ../user}}
						<a class="add-cart"><button style="background: none;border: none;" class="text-danger" onclick="addToCart('{{this._id}}')">+ Add To
								Cart</button></a>
						{{else}}
						<a href="/login" class="add-cart"><button style="background: none;border: none;" class="text-danger">+ Add To
								Cart</button></a>
						{{/if}}
						<div class="rating text-muted"><strike>
								{{this.actualPrice}}</strike>
							{{!-- <i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i> --}}
						</div>
						<h5>â‚¹{{this.offerPrice}}</h5>
						<div class="product__color__select">
							<span class="label text-light"
								style="font-size: 12px;box-shadow: rgba(0, 0, 0, 0.2) 0px 12px 28px 0px, rgba(0, 0, 0, 0.1) 0px 2px 4px 0px, rgba(255, 255, 255, 0.05) 0px 0px 0px 1px inset;background: #6676;">{{this.stock}}pcs
								Left</span>
						</div>
					</div>
				</div>

			</div>
			{{/each}}
			{{#each topPriced}}
			<div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix hot-sales">

				<div class="product__item">
					<div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
						<span class="label text-success"
							style="box-shadow: rgba(0, 0, 0, 0.15) 2.4px 2.4px 3.2px;">-{{this.productOffer}}%</span>
						<ul class="product__hover">

							{{#if ../user}}

							<li><a>
									<button style="background: none;border: none;" onclick="addToWishlist('{{this._id}}')">

										<img style="background: none;"
											src="data:image/png;base64,"
											alt="" data-pagespeed-url-hash="935450538"
											onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></button></a>
							</li>
							{{else}}
							<li><a  href="/login">



									<img 
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="935450538"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
								</a>
							</li>
							{{/if}}


							<li><a href="/product-view/{{this._id}}">

									<img style="background: none"
										src="data:image/png;base64,"
										alt="" data-pagespeed-url-hash="3756334862"
										onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li>
						</ul>
					</div>
					<div class="product__item__text">
						<h6>{{this.game}}</h6>
						{{#if ../user}}
						<a class="add-cart"><button style="background: none;border: none;" class="text-danger" onclick="addToCart('{{this._id}}')">+ Add To
								Cart</button></a>
						{{else}}
						<a href="/login" class="add-cart"><button style="background: none;border: none;" class="text-danger">+ Add To
								Cart</button></a>
						{{/if}}
						<div class="rating text-muted"><strike>
								{{this.actualPrice}}</strike>
							{{!-- <i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i>
							<i class="fa fa-star-o"></i> --}}
						</div>
						<h5>â‚¹{{this.offerPrice}}</h5>
						<div class="product__color__select">
							<span class="label text-light"
								style="font-size: 12px;box-shadow: rgba(0, 0, 0, 0.2) 0px 12px 28px 0px, rgba(0, 0, 0, 0.1) 0px 2px 4px 0px, rgba(255, 255, 255, 0.05) 0px 0px 0px 1px inset;background: #6676;">{{this.stock}}pcs
								Left</span>
						</div>
					</div>
				</div>

			</div>
			{{/each}}
			
		</div>
	</div>
</section>



<section class="latest spad">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div class="section-title">
					<span>Latest News</span>
					<h2></h2>
				</div>
			</div>
		</div>
		<div class="row">
			{{#each newFeed}}
			<div class="col-lg-4 col-md-6 col-sm-6 mt-1" id="blog_card">
				<div class="blog__item">
					<div class="blog__item__pic set-bg" data-setbg="{{this.image}}"></div>
					<div class="blog__item__text">
						<span><img src="img/icon/xcalendar.png.pagespeed.ic.GXy2gYWDh7.png" alt=""
								data-pagespeed-url-hash="2804280352"
								onload="pagespeed.CriticalImages.checkImageForCriticality(this);">{{this.displayDate}}</span>
						<h5>{{this.newsTitle}}</h5>
						<a href="{{this.link}}" target="_blank">Read More</a>
					</div>
				</div>
			</div>
			{{/each}}
			
		</div>
	</div>
</section>


